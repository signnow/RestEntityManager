{
    "name": "signnow/rest-entity-manager",
    "description": "Library gives you ability to communicate to REST API within DTO objects",
    "type": "library",
    "license": "MIT",
    "keywords": ["rest", "entity-manager", "signnow"],
    "autoload": {
        "psr-4": {
            "SignNow\\Rest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit",
            "Tests\\Fixtures\\": "tests/fixtures"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0.0",
        "doctrine/annotations": "~1.6",
        "signnow/serializer": "1.15",
        "ext-ctype": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.infrateam.xyz/"
        }
    ]
}
